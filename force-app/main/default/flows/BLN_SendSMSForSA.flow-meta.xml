    <description>FOUK-5537, This Flow is created for sending SMS when the Service Appointment status is updated to “Scheduled”</description>
    <environments>Default</environments>
    <formulas>
        <name>nosmsdate</name>
        <dataType>Boolean</dataType>
        <expression>CONTAINS({!$Label.BLN_NoSmsDate}, TEXT(DAY(DATEVALUE({!$Record.ArrivalWindowStartTime})))+&quot;-&quot;+TEXT(MONTH(DATEVALUE({!$Record.ArrivalWindowStartTime}))))</expression>
    </formulas>
    <interviewLabel>Send SMS For SA {!$Flow.CurrentDateTime}</interviewLabel>
