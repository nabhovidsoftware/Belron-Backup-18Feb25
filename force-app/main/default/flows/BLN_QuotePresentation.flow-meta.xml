                <operator>NotEqualTo</operator>
            </conditions>
            <conditions>
                <leftValueReference>recordId.BLN_ServiceGeolocation__Latitude__s</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <numberValue>0.0</numberValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>recordId.BLN_ServiceGeolocation__Longitude__s</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <numberValue>0.0</numberValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>recordId.BLN_SubType__c</leftValueReference>
