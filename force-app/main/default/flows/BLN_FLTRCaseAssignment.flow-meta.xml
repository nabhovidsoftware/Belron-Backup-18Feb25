    <start>
        <locationX>188</locationX>
        <locationY>0</locationY>
        <connector>
            <targetReference>Get_FLTR_Queue</targetReference>
        </connector>
        <filterFormula>AND
(
NOT(ISBLANK({!$Record.Subject})),
NOT(ISBLANK({!$Record.WhatId})), {!$Record.RecordType.DeveloperName} = &quot;BLN_ServiceTask&quot;,
{!$Record.Subject} = &quot;BVT&quot;, NOT(ISBLANK({!$Record.BLN_Case__r.Account.Id})), 
BEGINS({!$Record.WhatId}, &quot;500&quot;), ISPICKVAL({!$Record.BLN_Case__r.Account.BLN_FLTRIndicator__c}, &quot;FLTR&quot;) ,
OR(ISNEW(), ISCHANGED({!$Record.WhatId}), ISCHANGED({!$Record.Subject})),
NOT({!$Permission.BLN_BypassFlows})
)</filterFormula>
        <object>Task</object>
        <recordTriggerType>CreateAndUpdate</recordTriggerType>
        <triggerType>RecordAfterSave</triggerType>
    </start>
