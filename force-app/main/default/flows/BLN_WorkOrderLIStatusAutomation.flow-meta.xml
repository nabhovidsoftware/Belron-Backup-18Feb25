    <apiVersion>54.0</apiVersion>
    <description>FOUK-164--&gt;When Service Appointment Status Change then status of  Work Order Line Item also change.</description>
    <environments>Default</environments>
    <formulas>
        <name>CurrentUserId</name>
        <dataType>String</dataType>
        <expression>{!$User.Id}</expression>
    </formulas>
    <formulas>
        <name>WOLIStatusFormula</name>
        <dataType>String</dataType>
        <expression>CASE({!$Record.Status}, 
&quot;Open&quot;, &quot;New&quot;, 
&quot;Pending&quot;, &quot;New&quot;, 
&quot;Scheduled&quot;, &quot;Open&quot;, 
&quot;Dispatched&quot;, &quot;Open&quot;, 
&quot;Travel&quot;, &quot;In Progress&quot;, 
&quot;Arrived&quot;, &quot;In Progress&quot;, 
&quot;In Progress&quot;, &quot;In Progress&quot;, 
&quot;Completed&quot;, &quot;Completed&quot;, 
&quot;Failed After Arrive&quot;, &quot;Cannot Complete&quot;, 
&quot;Failed Before Arrive&quot;, &quot;Cannot Complete&quot;, 
&quot;Canceled&quot;, &quot;Canceled&quot;, 
&quot;New&quot;)</expression>
    </formulas>
