                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Stock Collect</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Loop_all_SA_records.Status</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Failed Before Arrive</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Loop_all_SA_records.Status</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Failed After Arrive</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_Failure_Reasons</targetReference>
            </connector>
            <label>Check SA status FAA or FBA</label>
        </rules>
    </decisions>
