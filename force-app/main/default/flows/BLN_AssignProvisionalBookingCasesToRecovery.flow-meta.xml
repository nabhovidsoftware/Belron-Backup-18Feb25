    <decisions>
        <description>FOUK-5368, This Decision Element is created for checking the Case Booking status value.</description>
        <name>BLN_CheckBookingStatus</name>
        <label>Check Booking Status</label>
        <locationX>1502</locationX>
        <locationY>276</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>BLN_BookingStatus</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.BLN_Case__r.BLN_BookingStatus__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Provisional</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.SchedStartTime</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <elementReference>entryConditionFormula</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>BLN_GetCaseRecords</targetReference>
            </connector>
            <label>Booking Status</label>
        </rules>
    </decisions>
