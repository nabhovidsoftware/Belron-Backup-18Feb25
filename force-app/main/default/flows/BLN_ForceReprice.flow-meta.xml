    <start>
        <locationX>1046</locationX>
        <locationY>0</locationY>
        <connector>
            <targetReference>Get_Records_Type</targetReference>
        </connector>
        <filterFormula>AND(
    OR(
        AND(
            ISCHANGED({!$Record.BLN_InsuranceSite__c}) ,
            NOT(ISNULL({!$Record__Prior.BLN_InsuranceSite__c}))
        ),

        AND(
            ISCHANGED({!$Record.BLN_CorporateSite__c}) ,
            NOT(ISNULL({!$Record__Prior.BLN_CorporateSite__c}))
        ),
OR(
ISCHANGED({!$Record.BLN_BodyGlassExcess__c}),
ISCHANGED({!$Record.BLN_RepairExcess__c}),
ISCHANGED({!$Record.BLN_ReplacementExcess__c}),
ISCHANGED({!$Record.BLN_HeatedRearExcess__c}),
ISCHANGED({!$Record.BLN_LimitIncludedInExcess__c}),
ISCHANGED({!$Record.BLN_RearWindscreenExcess__c}),
ISCHANGED({!$Record.BLN_ADASExcess__c}),
ISCHANGED({!$Record.BLN_VehicleDamageExcess__c}),
ISCHANGED({!$Record.BLN_WindscreenExcess__c}),
ISCHANGED({!$Record.BLN_PolicyLimit__c})
),

ISCHANGED({!$Record.BLN_VATRegisteredFlag__c})
    ),
    NOT({!$Permission.BLN_BypassFlows})
)</filterFormula>
        <object>BLN_Corporate__c</object>
        <recordTriggerType>Update</recordTriggerType>
        <triggerType>RecordAfterSave</triggerType>
    </start>
