            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>recordId.Type</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Feedback</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>ConfirmScreen</targetReference>
            </connector>
            <label>If case reopens and case type is Feedback</label>
        </rules>
