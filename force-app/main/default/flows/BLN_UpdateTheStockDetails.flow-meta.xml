    <start>
        <locationX>320</locationX>
        <locationY>0</locationY>
        <connector>
            <targetReference>Out_Of_Stock_Check</targetReference>
        </connector>
        <filterFormula>AND(
	NOT({!$Permission.BLN_BypassFlows}),
	NOT(ISBLANK({!$Record.Order.BLN_Case__c})),
	ISPICKVAL({!$Record.Order.BLN_Case__r.Type}, &apos;Job Request&apos;),
	ISPICKVAL({!$Record.Order.BLN_Case__r.BLN_SubType__c}, &apos;Job Request&apos;)
)</filterFormula>
        <object>OrderItem</object>
        <recordTriggerType>CreateAndUpdate</recordTriggerType>
        <triggerType>RecordAfterSave</triggerType>
    </start>
