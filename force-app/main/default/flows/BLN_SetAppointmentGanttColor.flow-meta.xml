    <description>FOUK-146,FOUK-53--&gt;This flow sets appointment gantt, color and label information.
FOUK-3831 --&gt; Added changes for Light Grey Color on Gantt for SA</description>
    <environments>Default</environments>
    <formulas>
        <name>AppointmentRebook</name>
        <dataType>Boolean</dataType>
        <expression>IF(NOT({!$Record.BLN_AppointmentRebook__c})&amp;&amp;TEXT({!$Record.BLN_JobComments__c})=&quot;Multiple Failed Appointments&quot;,
True,
{!$Record.BLN_AppointmentRebook__c})</expression>
    </formulas>
    <formulas>
        <name>CurrentUserID</name>
        <dataType>String</dataType>
        <expression>{!$User.Id}</expression>
    </formulas>
    <formulas>
        <name>GanttColors</name>
        <dataType>String</dataType>
        <expression>CASE(TEXT({!$Record.Status}),
&apos;Scheduled&apos;,&apos;#b3ffff&apos;,
&apos;Dispatched&apos;,&apos;#ACE8E2&apos;,
&apos;Travel&apos;,&apos;#FF8043&apos;,
&apos;Arrived&apos;,&apos;#B2FFB2&apos;,
&apos;In Progress&apos;,&apos;#B2FFB2&apos;,
&apos;Failed After Arrive&apos;,&apos;#ff3333&apos;,
&apos;Failed Before Arrive&apos;,&apos;#ff3333&apos;,
&apos;Completed&apos;,&apos;#B3B3B3&apos;,
&apos;&apos;)</expression>
    </formulas>
    <formulas>
        <name>NewGanttLabel</name>
        <dataType>String</dataType>
        <expression>{!$Record.Account.FirstName}+&apos; &apos;+{!$Record.Account.LastName}+{!Separator}+{!$Record.BLN_VRNInformation__c}</expression>
    </formulas>
    <formulas>
        <name>Separator</name>
        <dataType>String</dataType>
        <expression>IF(NOT(ISBLANK({!$Record.Account.LastName})) &amp;&amp;NOT(ISBLANK({!$Record.BLN_VRNInformation__c})),&apos; / &apos;,&apos;&apos;)</expression>
    </formulas>
