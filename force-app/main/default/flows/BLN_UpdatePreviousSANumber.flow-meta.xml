    <description>FOUK-11812 - Flow to update BLN_StocksPreviousAppointment (backend field) after 3 mins for only SA update scenario of stock movement</description>
    <environments>Default</environments>
    <formulas>
        <name>beforePreviousApp</name>
        <dataType>Boolean</dataType>
        <expression>IF(ISNULL({!Get_previous_SA.Id}), true, IF( {!$Record.BLN_ServiceAppointment__r.ArrivalWindowStartTime} &lt; {!Get_previous_SA.ArrivalWindowStartTime}  , true, false))</expression>
    </formulas>
    <formulas>
        <name>foStockOrderId</name>
        <dataType>String</dataType>
        <expression>{!$Record.Order.BLN_Case__r.CaseNumber} + {!$Label.BLN_FoStockOrderSeperator} + RIGHT({!$Record.BLN_ServiceAppointment__r.AppointmentNumber}, 6)</expression>
    </formulas>
    <formulas>
        <name>LineNumber</name>
        <dataType>String</dataType>
        <expression>{!$Record.BLN_ServiceAppointment__r.AppointmentNumber}+TEXT({!$Record.BLN_DocumentNumber__c})</expression>
    </formulas>
    <formulas>
        <name>outsideThreshold</name>
        <dataType>Boolean</dataType>
        <expression>IF(ISNULL({!Get_previous_SA.Id}), true, IF( {!$Record.BLN_ServiceAppointment__r.ArrivalWindowStartTime} &gt; ({!Get_previous_SA.ArrivalWindowStartTime} + {!$Record.BLN_BranchThreshold__c}) , true, false))</expression>
    </formulas>
    <formulas>
        <name>UpdateCondition</name>
        <dataType>Boolean</dataType>
        <expression>IF(ISNULL({!$Record.BLN_StocksPreviousAppointment__c}), true, 
IF(({!$Record.BLN_StocksPreviousAppointment__c}+TEXT({!$Record.BLN_DocumentNumber__c}))&lt;&gt;{!$Record.BLN_StockLineNumber__c}, true, false)
)</expression>
    </formulas>
