                <rightValue>
                    <stringValue>Job Request</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>recordId.BLN_SubType__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Job Request</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>recordId.BLN_SubType__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Warranty</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Confirm_Screen</targetReference>
            </connector>
            <label>If case reopens and case type is job request</label>
