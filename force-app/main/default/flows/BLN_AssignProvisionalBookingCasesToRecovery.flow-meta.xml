    <decisions>
        <name>Check_Case_Conditions</name>
        <label>Check Case Conditions</label>
        <locationX>314</locationX>
        <locationY>708</locationY>
        <defaultConnector>
            <targetReference>BLN_CaseAssignment</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Cash_Case</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>BLN_GetCaseRecords.BLN_OutstandingBalance__c</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <numberValue>0.0</numberValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>BLN_GetCaseRecords.BLN_BillingMethod__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Cash</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Copy_1_of_Case_Assignment</targetReference>
            </connector>
            <label>Cash Case</label>
        </rules>
        <rules>
            <name>Non_Cash_Case</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>BLN_GetCaseRecords.BLN_BookingStatus__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Provisional</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>BLN_GetCaseRecords.BLN_BillingMethod__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Cash</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Copy_2_of_Case_Assignment</targetReference>
            </connector>
            <label>Non Cash Case</label>
        </rules>
    </decisions>
