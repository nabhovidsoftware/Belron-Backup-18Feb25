    <description>FOUK-8101 Flow to check if case status is Closed-Completed then update the Order status</description>
    <environments>Default</environments>
    <formulas>
        <name>CheckCoverOfOP</name>
        <dataType>Boolean</dataType>
        <expression>IF(RIGHT({!Accepted_OP.BLN_TransactionNumber__c},1) = RIGHT({!Invoiced_OP.BLN_TransactionNumber__c},1),True,False)</expression>
    </formulas>
    <formulas>
        <name>LastDigitOfPreviousInvoicedOrderProduct</name>
        <dataType>String</dataType>
        <expression>RIGHT({!Previous_Invoiced_Order_s_Product.BLN_TransactionNumber__c},1)</expression>
    </formulas>
    <formulas>
        <name>LastDigitOfTransactionNumber</name>
        <dataType>String</dataType>
        <expression>RIGHT({!Loop_of_recent_accepted_order_s_products.BLN_TransactionNumber__c},1)</expression>
    </formulas>
    <formulas>
        <name>LastDigitOfTransactionNumber2</name>
        <dataType>String</dataType>
        <expression>RIGHT({!Loop_of_recent_accepted_order_s_product_2.BLN_TransactionNumber__c},1)</expression>
    </formulas>
    <formulas>
        <description>Updates transaction key by 10 at end</description>
        <name>NewTransactionKey</name>
        <dataType>String</dataType>
        <expression>LEFT({!Invoiced_OP.BLN_TransactionNumber__c}, FIND(&quot;/&quot;, {!Invoiced_OP.BLN_TransactionNumber__c})) &amp; 
TEXT({!Get_Accepted_Order.BLN_Case__r.BLN_RevisionNumber__c}) &amp;
IF({!Accepted_OP.BLN_TotalIncludingTaxByMotorist__c}!=0, &apos;4&apos;, IF({!Accepted_OP.BLN_TotalIncludingTaxInsuranceCompany__c}!=0,&apos;1&apos;,&apos;0&apos;))</expression>
    </formulas>
