        <scale>0</scale>
    </formulas>
    <formulas>
        <name>WarrantyReplacementRecalibrationFormula</name>
        <dataType>Number</dataType>
        <expression>IF(ISNULL({!$Record.ServiceTerritory.BLN_MeetAndGreetTime__c}), 0, {!$Record.ServiceTerritory.BLN_MeetAndGreetTime__c}) +
